<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="project"
            type="lu.aqu.projper.model.Project" />

    </data>

    <android.support.v7.widget.CardView
